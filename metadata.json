{
  "name": "igorolivei-nexus",
  "version": "1.0.0",
  "author": "igorolivei",
  "summary": "Puppet module to install Sonatype Nexus 3",
  "license": "Apache-2.0",
  "source": "https://github.com/igorolivei/puppet-nexus",
  "project_page": "https://github.com/igorolivei/puppet-nexus",
  "issues_url": "https://github.com/igorolivei/puppet-nexus/issues",
  "operatingsystem_support": [
